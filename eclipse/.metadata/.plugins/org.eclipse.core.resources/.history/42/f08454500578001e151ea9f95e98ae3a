package revise_pgm;

import java.util.ArrayList;

public class add2 {

	public static void main(String[] args) {
		ArrayList a=new 	ArrayList();
		a.add(26);
		a.add("hello");
		a.add(85);
		a.add('a');
		
		ArrayList a2=new 	ArrayList();
		a2.add(28);
		a2.add("hello java");
		a2.add(85);
		a2.add('b');
		System.out.println(a);
		System.out.println(a2);
		System.out.println(a.addAll(a2));
		
			}
			

}
