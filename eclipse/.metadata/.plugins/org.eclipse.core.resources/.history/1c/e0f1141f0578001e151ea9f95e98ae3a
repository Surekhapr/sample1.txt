package revise_pgm;

import java.util.ArrayList;

public class add2 {

	public static void main(String[] args) {
		ArrayList a=new 	ArrayList();
		a.add(26);
		a.add("hello");
		a.add(85);
		a.add('a');
		System.out.println(a.get(1));
		Object[] o=a.toArray();
		for( int i=0;i<o.length;i++)
			{
			System.out.println(o[i]);
			
			}
			}
			

}
